<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
       PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
       "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kf.plt.internship.ssm.service.mybatis.mapper.UserMapper">
	 <select id="selectUser" resultType="kf.plt.internship.ssm.service.mybatis.bean.User">
		select * from user
	 </select>
	 <select id="selectUserById" resultType="kf.plt.internship.ssm.service.mybatis.bean.User">
		select * from user where id = #{id}
	</select>
</mapper>
